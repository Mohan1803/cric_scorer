{
  "expo": {
    "name": "Cric Scorer",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cric.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "package": "com.boltexpo.cricket",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cric.png",
        "backgroundColor": "#ffffff"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.boltexpo.cricket"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/cric.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "70c62b4f-9fb0-4e6c-bfc7-08a8ef9aa8b2"
      }
    }
  }
}
